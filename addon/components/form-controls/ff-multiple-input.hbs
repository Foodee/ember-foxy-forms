{{#each this._inputs as |item|}}
  <input
    class="input-{{item.id}}"
    type="text"
    value={{get @value item.key}}
    disabled={{@disabled}}
    ...attributes
    data-test-multiple-input
    {{on "change" (fn this.handleValueChange item.key)}}
    {{on "keyup" (fn this.handleValueChange item.key)}}
  />
{{/each}}