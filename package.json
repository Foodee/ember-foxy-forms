{
  "name": "ember-foxy-forms",
  "version": "2.17.0",
  "description": "An addon for building forms that work with ember data",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/Foodee/ember-foxy-forms",
  "license": "MIT",
  "author": "",
  "homepage": "https://foodee.github.io/ember-foxy-forms",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "ember-arg-types": "^0.2.1",
    "ember-attacher": "1.1.1",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-composable-helpers": "^4.3.0",
    "ember-element-helper": "^0.3.1",
    "ember-event-helpers": "^0.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.22.0",
    "ember-cli-addon-docs": "^0.10.0",
    "ember-cli-addon-docs-esdoc": "^0.2.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-data": "^3.22.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-inputmask": "^0.11.0",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-template-lint-plugin-prettier": "^2.0.0",
    "ember-test-selectors": "^5.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.1.0",
    "loader.js": "^4.7.0",
    "minimatch": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "qunit-dom": "^1.5.0",
    "sinon": "^9.2.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
